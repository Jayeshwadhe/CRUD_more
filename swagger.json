{
  "swagger": "2.0",

  "info": {
    "version": "1.0.0",

    "title": "userRegistration",

    "license": {
      "name": "Employee Registration"
    }
  },

  "host": "localhost:3000",

  "schemes": ["http", "https"],

  "produces": ["application/json"],

  "securityDefinitions": {
    "APIKeyHeader": {
      "type": "apiKey",

      "in": "header",

      "name": "authenticate"
    }
  },
  "security": [
    {
      "APIKeyHeader": []
    }
  ],
  "paths": {
    "/register": {
      "post": {
        "summary": "Employee Register",

        "tags": ["Employee"],

        "description": "Employee Registration",

        "parameters": [
          {
            "name": "Employee Registration",

            "in": "body",

            "description": "Employee Registration",

            "schema": {
              "properties": {
                "name": {
                  "type": "string"
                },

                "email": {
                  "type": "string"
                },

                "password": {
                  "type": "string"
                },

                "phoneno": {
                  "type": "string"
                },

                "salary": {
                  "type": "number"
                },

                "gender": {
                  "type": "string"
                },

                "birthday": {
                  "type": "string"
                },

                "city": {
                  "type": "string"
                }
              }
            }
          }
        ],
        "responses": {
          "403": {
            "description": "Access Denied"
          },

          "404": {
            "description": "please login again!"
          },

          "401": {
            "description": "This student already exists"
          },

          "200": {
            "description": "Login Success"
          },

          "500": {
            "description": "Something went wrong"
          }
        }
      }
    }
  },
  "/login": {
    "post": {
      "summary": "Employee Register",

      "tags": ["Employee"],

      "description": "Employee Registration",

      "parameters": [
        {
          "name": "Employee Registration",

          "in": "body",

          "description": "Employee Registration",

          "schema": {
            "properties": {              

              "email": {
                "type": "string"
              },

              "password": {
                "type": "string"
              }

               }
          }
        }
      ],
      "responses": {
        "403": {
          "description": "Access Denied"
        },

        "404": {
          "description": "please login again!"
        },

        "401": {
          "description": "This student already exists"
        },

        "200": {
          "description": "Login Success"
        },

        "500": {
          "description": "Something went wrong"
        }
      }
    }
  }
}
  

