{
  "openapi": "3.0.0",
  "info": {
    "title": "Sample API",
    "description": "Employee Registration API",
    "version": "0.1.9"
  },
  "servers": [
    {
      "url": "http://localhost:3000/"
    }
  ],
  "paths": {
    "/Register": {
      "post": {
        "summary": "Employee Register",
        "description": "OK (successfully register)",
        "parameters": [
          {
            "name": "Employee Registration",
            "in": "body",
            "description": "Employee Registration",
            "schema": {
              "properties": {
                "name": {
                  "type": "string"
                },
                "email": {
                  "type": "string"
                },
                "password": {
                  "type": "string"
                },
                "phoneno": {
                  "type": "string"
                },
                "salary": {
                  "type": "number"
                },
                "gender": {
                  "type": "string"
                },
                "birthday": {
                  "type": "string"
                },
                "city": {
                  "type": "string"
                }
              }
            }
          }
        ],
        "responses": {
          "403": {
            "description": "Access Denied"
          },

          "404": {
            "description": "please login again!"
          },

          "401": {
            "description": "This student already exists"
          },

          "200": {
            "description": "Login Success"
          },

          "500": {
            "description": "Something went wrong"
          }
        }
      }
    },
    "/login": {
      "post": {
        "summary": "Employee Register",

        "tags": ["Employee"],

        "description": "Employee Registration",

        "parameters": [
          {
            "name": "Employee Registration",

            "in": "body",

            "description": "Employee Registration",

            "schema": {
              "properties": {
                "email": {
                  "type": "string"
                },

                "password": {
                  "type": "string"
                }
              }
            }
          }
        ],
        "responses": {
          "403": {
            "description": "Access Denied"
          },

          "404": {
            "description": "please login again!"
          },

          "401": {
            "description": "This student already exists"
          },

          "200": {
            "description": "Login Success"
          },

          "500": {
            "description": "Something went wrong"
          }
        }
      }
    },
    "/EmployeeDetails": {
      "get": {
        "security": [
          {
            "- basicAuth": []
          }
        ],
        "summary": "get all user",
        "tags": ["User"],
        "description": "get all user",
        "parameters": [],
        "responses": {
          "403": {
            "description": "Forbidden"
          },
          "404": {
            "description": "success"
          },
          "401": {
            "description": "This user already exists"
          },
          "200": {
            "description": " User Login Success"
          },
          "500": {
            "description": "Internal Server Error"
          },
          "505": {
            "description": "HTTP Version Not Support"
          }
        }
      }
    }
  }
}
